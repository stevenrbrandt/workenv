sub file {
  my $file = shift;
  if($file =~ /^\//) {
    return " -url 'file://$file'";
  } else {
    my $cw = Cwd::getcwd();
    return " -url 'file://$cw/$file'";
  }
}

my $cmd;
my $b1 = "/mnt/c/Program Files (x86)/Google/Chrome/Application/chrome.exe";
my $b2 = "/mnt/c/Users/sbrandt/AppData/Local/Programs/Opera/launcher.exe";
my $b3 = "/mnt/c/Program Files/Google/Chrome/Application/chrome.exe";
if(-e $b1) {
    $cmd = "'$b1'";
}
if(-e $b3) {
    $cmd = "'$b3'";
}
if(-e $b2) {
    $cmd = "'$b2'";
}
die unless(defined($cmd));
#"google-chrome"
#"/home/sbrandt/Apps/firefox/firefox"
;
for(my $i=0;$i <= $#ARGV;$i++) {
    my $a = $ARGV[$i];
    if(defined($bookmarks{$a})) {
        $cmd .= " -url '$bookmarks{$a}->[0]'";
    } elsif($a eq "-u") {
        $cmd .= " -url ".$ARGV[++$i];
    } elsif($a eq "-f") {
        my $cwd = Cwd::getcwd();
        my $file = $ARGV[++$i];
        if($file !~ /^\//) {
            $file = $cwd."/".$file;
            $file =~ s/\/+/\//g;
        }
        print("file=$file\n");
        my $whome;
        if(-r "/mnt/c/Users/sbrandt") {
            $whome = "file:///C:/Users/sbrandt";
        } else {
            $whome = "file:///C:/Users/steve";
        }
        if($file =~ s{.*/Downloads}{$whome/Downloads}) {
            $cmd .= " -url '$file'";
        } elsif($file =~ "/mnt/c/Users") {
            $cmd .= " -url 'file:///C:/Users$''";
        } else {
            exit(0);
        }
    } elsif($a =~ /-f=(.*)/) {
        $cmd .= file($1);
    } else {
        usage();
        die "No such bookmark: $a";
    }
}
if($#ARGV < 0) {
    usage();
} else {
    print "cmd=$cmd\n";
    system("$cmd 2>&1 1>/tmp/google.log &");
}

sub usage {
    for my $k (sort keys %bookmarks) {
        printf("%50s|%10s\n",$bookmarks{$k}->[1],$k);
    }
}
