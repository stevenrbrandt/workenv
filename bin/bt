#!/usr/bin/perl
# in gdb: "list *address"
# if clang was used: "llvm-symbolizer -e myprogram 0x804d5ce"
while(<>) {
    if(m{([\w/\.]*/cactus_[-\w]+).*[\[+]0x([a-fA-F0-9]+)[\])]}) {
        my ($exe,$addr)=($1,$2,$3);
        print("addr2line -e $exe -f -p $addr\n");
        system("addr2line -e $exe -f -p $addr");
    }
}
